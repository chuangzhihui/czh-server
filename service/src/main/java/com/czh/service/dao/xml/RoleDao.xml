<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.czh.service.dao.RoleDao">

    <resultMap type="com.czh.service.entity.Role" id="RoleMap">
        <result property="roleId" column="roleId" jdbcType="INTEGER"/>
        <result property="roleName" column="roleName" jdbcType="VARCHAR"/>
        <result property="ids" column="ids" jdbcType="VARCHAR"/>
        <result property="describe" column="describe" jdbcType="VARCHAR"/>
        <result property="atime" column="atime" jdbcType="TIMESTAMP"/>
    </resultMap>
    <select id="getRoleSelectList" resultType="com.czh.common.vo.SelectVo">
        select roleId as value ,roleName as label from czh_role
    </select>


</mapper>

